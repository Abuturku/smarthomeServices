<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
											http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<jaxws:endpoint id="outsideTemperatureService"
		implementor="de.mosbach.lan.smarthome.services.OutsideTemperatureService" address="/outsideTemperatureService" />
		
	<jaxws:endpoint id="windowService"
		implementor="de.mosbach.lan.smarthome.services.WindowService" address="/windowService" />
	
	<jaxws:endpoint id="heaterService"
		implementor="de.mosbach.lan.smarthome.services.HeaterService" address="/heaterService" />
		
	<jaxws:endpoint id="airConditionerService"
		implementor="de.mosbach.lan.smarthome.services.AirConditionerService" address="/airConditionerService" />
		
	<jaxws:endpoint id="InsideTemperatureService"
		implementor="de.mosbach.lan.smarthome.services.InsideTemperatureService" address="/insideTemperatureService" />

</beans>
